{
    "app": {
        "version": "1.0.0-enterprise",
        "theme": "dark",
        "autosave_config": true,
        "window_size": [
            1200,
            800
        ]
    },
    "crawler": {
        "mode": "pagination",
        "max_concurrency": 10,
        "request_timeout": 10,
        "retry": 3,
        "follow_robots": false,
        "enable_playwright": true,
        "max_depth": 3,
        "delay_range": [
            0.5,
            1.5
        ]
    },
    "pagination": {
        "selector": ".next a",
        "attribute": "href",
        "limit": 50
    },
    "domain_crawler": {
        "enabled": false,
        "same_domain_only": true,
        "max_pages": 300,
        "exclude_extensions": [
            ".jpg",
            ".png",
            ".jpeg",
            ".gif",
            ".svg",
            ".webp",
            ".pdf",
            ".zip"
        ]
    },
    "login": {
        "enabled": false,
        "type": "form",
        "form_url": "",
        "username_field": "",
        "password_field": "",
        "username": "",
        "password": "",
        "cookie": ""
    },
    "selectors": {
        "data_selector": ".product_pod",
        "title_selector": "title",
        "content_attribute": "title"
    },
    "proxy": {
        "enabled": false,
        "rotate": true,
        "proxy_list": [],
        "proxy_api": ""
    },
    "export": {
        "export_csv": true,
        "export_json": true,
        "export_sqlite": true,
        "output_directory": "output"
    }
}
